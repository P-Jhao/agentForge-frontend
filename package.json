{
  "name": "agentforge-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint src --fix",
    "format": "prettier --write src",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "naive-ui": "^2.43.2",
    "vue": "^3.5.24"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^24.10.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "^10.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.34.0",
    "vite": "^7.2.4",
    "vue-tsc": "^3.1.4"
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,md,css,scss,html}": [
      "prettier --write"
    ]
  }
}
